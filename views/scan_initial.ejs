<% include ./partials/header %>

<div class="container">
        <div class="row">
            <h1 style="text-align: center">Scan</h1>
            <div style="width:30%; margin: 25px auto;">
            <form action="/scan" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text" name="info[file]" placeholder="List Name">
                </div>
                <div class="form-group">
                   
                   <% for(var i=0;i<schemas.length;i++){ %>
                       <% if(schemas[i] !== 'Mlist' && schemas[i] !== 'Info'){ %>
                        <li><%= schemas[i] %></li>
                   <% }}; %>           
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-default btn-block btn-primary">Submit!</button>
                </div>
                <a href="/home">Go Back</a>
           </form>    
        </div>
    </div>
</div>


<% include ./partials/footer %>
